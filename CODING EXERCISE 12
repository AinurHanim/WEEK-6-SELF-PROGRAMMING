package SelfProgramming;

import java.util.Scanner;

public class Switch {

	public static void main(String[] args) {
		//Use scanner input, 
		//Enter integer number. 
		//Jika  number sama dengan input number, 
		//print number tersebut
		//Use scanner input, 
		//Enter integer number. 
		//Jika  number 20 sama dengan input number, 
		//print number tersebut 10 20 30
		//Katakan kamu ada 3 number iaitu 10, 20 dan 30. /
		//Semak dengan input number
		int num;
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Enter the integer number ; ");
		num = scan.nextInt();
		
		if(num==10)
		{
			System.out.println("Number is 10");
		}
		else if(num==20)
		{
			System.out.println("Number is 20");
		}
		else if(num==30)
		{
			System.out.println("Number is 30");
		}
		else
		{
			System.out.println("Invalid number");
		}

	}

}
