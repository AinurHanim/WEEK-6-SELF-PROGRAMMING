package SelfProgramming;

import java.util.Scanner;

public class Exercise3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String sub1;
		String sub2;
		double marks1;
		double marks2;
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Your first subject is ");
		sub1 = scan.next();		
		System.out.println("Your mark is ");
		marks1 = scan.nextDouble();
		
		System.out.println("Your  second subject is ");
		sub2 = scan.next();		
		System.out.println("Your mark is ");
		marks2 = scan.nextDouble();
		
		double avg = (marks1 + marks2)/2;
		
		if (marks1>marks2)
			System.out.println("Subject " +sub1 + " " +marks1);
		else 
			System.out.println("Subject " +sub2 + " " +marks2);
		if ((marks1 > 50) && (marks2 > 50))
			System.out.println("PASS");
		else
			System.out.println("FAIL");
	}

}
